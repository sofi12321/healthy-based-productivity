    "def parse_data(input_string):\n",
    "    pattern_1 = r'\\d+\\. [A-z ]+\\n'\n",
    "    pattern_2 = r\"[A-z ]+\"\n",
    "    string = (input_string + \"\\n\").split(\".\\n\")[0] + \"\\n\"\n",
    "    result = [re.findall(pattern_2, x)[0][1:] for x in re.findall(pattern_1, string)]\n",
    "    return result"
   ]
  },
