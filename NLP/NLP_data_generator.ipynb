    "def update_array(x, y, z):\n",
    "    for element in z:\n",
    "        flag = True\n",
    "        element = element.lower()\n",
    "        for key in x.keys():\n",
    "            if element in x[key]:\n",
    "                flag = False\n",
    "        if flag:\n",
    "            x[y].append(element)"
   ]
