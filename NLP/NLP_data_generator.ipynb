   "source": [
    "epochs = 10\n",
    "api_key = \input()\n",
    "model = \"gpt-3.5-turbo-0301\"\n",
    "# requests-per-minute = 60\n",
    "# tokens-per-minute = 150\n",
    "for i in range(epochs):\n",
    "    for key in data.keys():\n",
    "        if amount[key] != 0:\n",
    "            context = contexts[key]\n",
    "            base = re.sub(\"AMOUNT\", str(amount[key]), base_query)\n",
    "            end = end_query\n",
    "            for j in range(3):\n",
    "                end += \"\\n\" + str(j+1) + \". \" + data[key][j]\n",
    "            if len(tasks[key]) != 0:\n",
    "                task = words_to_text(tasks[key])\n",
    "                query = re.sub(\"TASKS\", task, re.sub(\"CONTEXT\", context, \n",
    "                                                     base[:-1] + additional_query + end))\n",
    "            else:\n",
    "                query = re.sub(\"CONTEXT\", context, base + end)\n",
    "            answer = ask_GPT(query, api_key, model = model)\n",
    "            time.sleep(60)\n",
    "            tmp = parse_data(answer)\n",
    "            update_array(data, key, tmp)"
   ]
