   "source": [
    "def add_label(data, label):\n",
    "    data_copy = copy.deepcopy(data)\n",
    "    for i, j in enumerate(data_copy):\n",
    "        data_copy[i] = (j, label)\n",
    "    data_copy = np.array(data_copy)\n",
    "    return data_copy"
   ]
  },
