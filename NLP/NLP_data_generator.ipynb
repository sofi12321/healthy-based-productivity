    "def words_to_text(words):\n",
    "    text = \"\"\n",
    "    for word in words[:-1]:\n",
    "        text += (\"'\" + word.lower() + \"', \")\n",
    "    text = text[:-2] + \" or '\" + words[-1].lower() + \"'\"\n",
    "    return text"
   ]
  },
